<%- include("../partials/header") %>
<div class="container">
	<h1 class="text-center p-2">
	Add New Photos To The Gallery
</h1>

<form  action="/campgrounds" method="post">
	<div style="width:300px;margin:0 auto">
		<div class="form-group">
			<input class="form-control" type="text" name="name" placeholder="text">
		</div>
		<div class="form-group">
			<input class="form-control" type="url" name="img" placeholder="url">
		</div>
			<div class="form-group">
			<input class="form-control" type="text" name="discription" placeholder="discription">
		</div>
	<div class="form-group">
		<button class="btn btn-primary btn-sm">
		submit
	</button>
		<button class="btn btn-default btn-sm">
			<a href="/campgrounds">Back</a>
		</button>
		</div>
</div>
	
</form>
	
</div>


<%- include("../partials/footer") %>